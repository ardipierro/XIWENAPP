{
  "name": "xiwenapp-v2",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "emulators": "firebase emulators:start",
    "test:seed": "node test/seed-test-data.js",
    "test:functions": "node test/test-functions.js",
    "test:local": "npm run test:seed && npm run test:functions",
    "functions:install": "cd functions && npm install",
    "functions:deploy": "firebase deploy --only functions",
    "migrate:unified-content": "node scripts/migrateToUnifiedContent.js"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "firebase": "^12.4.0",
    "idb": "^8.0.3",
    "livekit-client": "^2.15.14",
    "lucide-react": "^0.552.0",
    "perfect-freehand": "^1.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.5",
    "recharts": "^3.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^1.1.0"
  }
}
